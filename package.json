{
  "name": "@carbonmint/reactml",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:ssl": "node server.js",
    "dev": "next dev",
    "build": "npm run clean-build",
    "build:clean": "rm -rf .next gen public/specs tools/build specs/gen specs/gen-yaml specs/workflows/gen static/jsonschemas",
    "build:next": "node --max-old-space-size=8192 node_modules/.bin/next build",
    "build:compile-wf": "node_modules/typescript/bin/tsc --project tsconfig.wf.json",
    "build:sc": "cd smart-contracts && yarn && yarn build",
    "next": "next build",
    "build:setup": "npm i -g coffeescript && mkdir -p gen-yaml gen/yaml gen/pages specs/build gen/jsonschemas",
    "clean": "npm run build:clean",
    "clean-build": "npm run build:clean && npm run build:setup && npm run build:sc && npm run codegen && npm run build:next",
    "copy-fake-to-mongo": "node_modules/typescript/bin/tsc tools/fakedb2mongodb.ts --resolveJsonModule --esModuleInterop --allowJs -outDir build && node build/tools/fakedb2mongodb.js",
    "start": "next start",
    "lint": "next lint",
    "codegen:watch": "mkdir -p gen-yaml && node tools/watch-codegen.js",
    "codegen": "mkdir -p gen-yaml && node tools/build-json-schemas && node tools/codegen.js && cp ./static/WorkflowDef.json ./gen/jsonschemas",
    "test": "ts-mocha -r tsconfig-paths/register --timeout 50000",
    "check-types": "tsc --pretty --noEmit",
    "eslint:check": "eslint '**/*.{js,jsx,ts,tsx}'",
    "eslint:fix": "eslint --fix '**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,css,md,json}'",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "pre-push": "lint-staged && npm run clean-build && rm -rf dist",
    "setupOrgLib": "node setupOrgLibAccess"
  },
